<h1>Your dashboard</h1>
<form>
    <input type="text" placeholder="Blog Title">
    <textarea placeholder="Your content here"></textarea>
    <button type="submit">Create Post</button>
</form>
<div>
    {{#each user.blogPosts}}
    <div>
        <h2>{{this.title}}</h2>
        <p>{{this.content}}</p>
        <button data-id="{{this.id}}">Edit Post</button>
        <button data-id="{{this.id}}">Delete Post</button>
    </div>
    {{/each}}
</div>